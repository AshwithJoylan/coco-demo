{
  "env": {
    "development": {
       "plugins": [
          [
            "module-resolver",
            
            {
              "cwd": "babelrc",
              "root": [
                "./src"
              ],
              "extensions": [
                ".js",
                ".ts",
                ".tsx",
                ".ios.js",
                ".android.js"
              ],
              "alias": {
                "@config": "./src/config",
                "@i18n": "./src/i18n",
                "@features": "./src/features",
                "@components": "./src/components",
                "@commons": "./src/components/commons",
                "@constants": "./src/constants",
                "@images": "./src/images",
                "@navigation": "./src/navigation",
                "@screens": "./src/screens",
                "@services": "./src/services",
                "@store": "./src/store",
                "@types": "./src/types",
                "@firebase": "./src/firebase"
              }
            }
          ]
      ]
    },
    "production": {
      "plugins": ["transform-remove-console",[
            "module-resolver",
            
            {
              "cwd": "babelrc",
              "root": [
                "./src"
              ],
              "extensions": [
                ".js",
                ".ts",
                ".tsx",
                ".ios.js",
                ".android.js"
              ],
              "alias": {
                "@features": "./src/features",
                "@firebase": "./src/firebase",
                "@i18n": "./src/i18n",
                "@hooks": "./src/hooks",
                "@components": "./src/components",
                "@constants": "./src/constants",
                "@images": "./src/images",
                "@navigators": "./src/navigators",
                "@screens": "./src/screens",
                "@services": "./src/services",
                "@store": "./src/store",
                "@database": "./src/database",
                "@types": "./src/types",
                "@utils": "./src/utils",
                "@commons": "./src/commons"
              }
            }
          ]]
    }
  }
}